  },
  "programId": {
    "name": "programId",
    "in": "path",
    "description": "Uniquely identifies an academic program",
    "required": true,
    "type": "string",
    "pattern": "^[0-9]{5}$"
  },
  "titleCode": {
    "name": "titleCode",
    "in": "path",
    "description": "title code of course",
    "required": true,
    "type": "string",
    "pattern": "^[0-9]{3}$"
  }
