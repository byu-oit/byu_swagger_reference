{
  "academicUnitId": {
    "name": "Academic Unit Identifier",
    "in": "path",
    "description": "Uniquely identifies an academic unit",
    "required": true,
    "type": "string",
    "pattern": "^[0-9]{4}$"
  },
  "curriculumId": {
    "name": "Curriculum Unit Identifier",
    "in": "path",
    "description": "Uniquely identifies a curriculum unit",
    "required": true,
    "type": "string",
    "pattern": "^[0-9]{5}$"
  },
  "programId": {
    "name": "Academic Program Identifier",
    "in": "path",
    "description": "Uniquely identifies an academic program",
    "required": true,
    "type": "string",
    "pattern": "^[0-9]{5}$"
  },
  "titleCode": {
    "name": "Title Code",
    "in": "path",
    "description": "Uniquely identifies a course title within a curriculum unit",
    "required": true,
    "type": "string",
    "pattern": "^[0-9]{3}$"
  }
}