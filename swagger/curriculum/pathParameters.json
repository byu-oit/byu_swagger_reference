{
  "curriculumId": {
    "name": "curriculumId",
    "in": "path",
    "description": "Uniquely identifies a curriculum unit",
    "required": true,
    "type": "string",
    "pattern": "^[0-9]{5}$"
  },
  "programId": {
    "name": "programId",
    "in": "path",
    "description": "Uniquely identifies an academic program",
    "required": true,
    "type": "string",
    "pattern": "^[0-9]{5}$"
  },
  "titleCode": {
    "name": "titleCode",
    "in": "path",
    "description": "Uniquely identifies a course title within a curriculum unit",
    "required": true,
    "type": "string",
    "pattern": "^[0-9]{3}$"
  }
}